{
    "manifestVersion": 1,
    "id": "pulumi-azure-pipelines-task",
    "name": "Pulumi Azure Pipelines Task",
    "version": "0.1.0",
    "publisher": "pulumi",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Build/release task for running Pulumi apps in Azure Pipelines.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/pulumi_mark_flat_on_light_128x128.png"
    },
    "files": [
        {
            "path": "buildAndReleaseTask"
        }
    ],
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildAndReleaseTask"
            }
        }
    ]
}
