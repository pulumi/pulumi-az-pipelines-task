{
    "manifestVersion": 1,
    "id": "pulumi-azure-pipelines-task",
    "name": "Pulumi Azure Pipelines Task",
    "version": "0.1.3",
    "publisher": "pulumi",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Build/release task for running Pulumi apps in Azure Pipelines.",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/pulumi_mark__on_dark_128x128.png"
    },
    "files": [
        {
            "path": "buildAndReleaseTask"
        }
    ],
    "links": {
        "home": {
            "uri": "https://www.pulumi.com"
        },
        "getstarted": {
            "uri": "https://pulumi.io/quickstart/"
        },
        "learn": {
            "uri": "https://www.pulumi.io"
        },
        "support": {
            "uri": "https://slack.pulumi.io"
        },
        "repository": {
            "uri": "https://github.com/pulumi/pulumi-az-pipelines-task"
        },
        "issues": {
            "uri": "https://github.com/pulumi/pulumi-az-pipelines-task/issues"
        }
    },
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildAndReleaseTask"
            }
        }
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/pulumi/pulumi-az-pipelines-task"
    },
    "branding": {
        "color": "#522768",
        "theme": "dark"
    },
    "content": {
        "details": {
            "path": "overview.md"
        }
    }
}
